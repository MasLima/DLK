	KEY ALT+A, "" ;
	MESSAGE "Crea, abre, guarda o imprime archivos, o sale de Visual FoxPro"
DEFINE PAD _0gw12el8f OF _MSYSMENU PROMPT "\<Movimientos" COLOR SCHEME 3 ;
	KEY ALT+M, ""
